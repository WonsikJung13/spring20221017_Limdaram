<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.example.spring20221017.mapper">
	<select id="getProductList" resultType="com.example.spring20221017.domain.JavaBean06">
	SELECT 
		ProductName name,
		Price
	FROM
		Products
	</select>

	<select id="getEmployeeList" resultType="com.example.spring20221017.domain.JavaBean05">
		SELECT
		firstName,
		lastName
		FROM
		Employees

	</select>
</mapper>